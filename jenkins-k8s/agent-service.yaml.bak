apiVersion: v1
kind: Service
metadata:
  labels:
    app: my-jenkins
  name: my-jenkins-agent
  namespace: default
spec:
  ports:
  - name: slavelistener
    port: 50000
    protocol: TCP
    targetPort: 50000
  selector:
    component: my-jenkins-jenkins-master
  sessionAffinity: None
  type: ClusterIP
